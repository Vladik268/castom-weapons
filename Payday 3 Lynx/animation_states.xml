<?xml version="1.0"?>



<tweaks>
	<tweak version="2" name="anims/fps/fps_spawn" extension="animation_states">
		<search>
			<xml/>
			<state name="fps/reload"/>
		</search>
		<target mode="append">
			<state name="fps/reload/lynx" type="once" segment="base" speed="1.0">
				<anim name="lynx_reload"/>
				<keys>
					<key at="trigger" trigger="het5_start_foley" callback="play_sound" class_name="base" param1="lynx_start"/>
					<key at="trigger" trigger="het5_mag_release" callback="play_sound" class_name="base" param1="lynx_mag_release"/>
					<key at="trigger" trigger="het5_magout" callback="play_sound" class_name="base" param1="lynx_mag_out"/>
					<key at="trigger" trigger="bullet_show" callback="anim_clbk_check_bullet_object" class_name="base" />
					<key at="trigger" trigger="shepheard_clip_handle" callback="play_sound" class_name="base" param1="pd3_lynx_mag_handle"/>
					<key at="trigger" trigger="het5_mag_contact" callback="play_sound" class_name="base" param1="pd3_lynx_mag_contact"/>
					<key at="trigger" trigger="het5_magin" callback="play_sound" class_name="base" param1="pd3_lynx_mag_in"/>
					<key at="trigger" trigger="het5_hand_swap" callback="play_sound" class_name="base" param1="pd3_lynx_hand_swap"/>
					<key at="trigger" trigger="het5_bolt_back" callback="play_sound" class_name="base" param1="pd3_lynx_bolt_back"/>
					<key at="trigger" trigger="het5_bolt_forward" callback="play_sound" class_name="base" param1="pd3_lynx_bolt_forward"/>
					<key at="trigger" trigger="het5_end_foley" callback="play_sound" class_name="base" param1="pd3_lynx_end"/>
								
					


				</keys>
				<exit name="fps/idle"/>
			</state>
			
			
			<state name="fps/reload_not_empty/lynx" type="once" segment="base" speed="1.0">
				<anim name="lynx_reload_not_empty"/>
				<keys>
					<key at="trigger" trigger="het5_start_foley" callback="play_sound" class_name="base" param1="lynx_start"/>
					<key at="trigger" trigger="het5_mag_release" callback="play_sound" class_name="base" param1="lynx_mag_release"/>
					<key at="trigger" trigger="het5_magout" callback="play_sound" class_name="base" param1="lynx_mag_out"/>
					<key at="trigger" trigger="bullet_show" callback="anim_clbk_check_bullet_object" class_name="base" />
					<key at="trigger" trigger="shepheard_clip_handle" callback="play_sound" class_name="base" param1="pd3_lynx_mag_handle"/>
					<key at="trigger" trigger="het5_mag_contact" callback="play_sound" class_name="base" param1="pd3_lynx_mag_contact"/>
					<key at="trigger" trigger="het5_magin" callback="play_sound" class_name="base" param1="pd3_lynx_mag_in"/>
					<key at="trigger" trigger="het5_hand_swap" callback="play_sound" class_name="base" param1="pd3_lynx_hand_swap"/>
					<key at="trigger" trigger="het5_mag_rattle" callback="play_sound" class_name="base" param1="het5_mag_rattle"/>
					<key at="trigger" trigger="het5_mag_slap" callback="play_sound" class_name="base" param1="het5_mag_slap"/>
					<key at="trigger" trigger="het5_end_foley" callback="play_sound" class_name="base" param1="pd3_lynx_end"/>

				</keys>
				<exit name="fps/idle"/>
			</state>
		</target>
	</tweak>
	
	<tweak version="2" name="anims/fps/fps_spawn" extension="animation_states">
		<search>
			<xml/>
			<state name="fps/equip"/>
			<keys/>
            		<key at="trigger" trigger="peacemaker_equip" callback="play_sound" class_name="base" param1="pmkr45_equip"/>
		</search>
		<target mode="append">
			<key at="trigger" trigger="het5_sextend" 	callback="play_sound" class_name="base" param1="het5_extend"/>
			<key at="trigger" trigger="het5_end_foley" 	callback="play_sound" class_name="base" param1="pd3_lynx_end"/>
		</target>
	</tweak>
	
	<tweak version="2" name="anims/fps/fps_spawn" extension="animation_states">
		<search>
			<xml/>
			<state name="fps"/>
		</search>
		<target mode="attach">
			<to name="reload_lynx" 						redirect="fps/reload/lynx"/>
			<to name="reload_not_empty_lynx" 			redirect="fps/reload_not_empty/lynx"/>
			<to name="equip_lynx" 						redirect="fps/equip/lynx"/>
		</target>
	</tweak>
	
